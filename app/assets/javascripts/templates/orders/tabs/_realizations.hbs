<div class="col-md-12">
  <label class="title">מימושים</label>
  <div class="row">
    <div class="col-md-12">
      {{#each realized in realizations}}
        <div class="row">
          <div class="col-xs-4">
            <div class="form-group">
              <label>שם ספק</label>
              {{input class="form-control" value=realized.supplierName readonly="readonly"}}
            </div>
          </div>
        </div>
        {{#each item in  realized.items}}
              <div class="row">
               <div class="col-xs-3">
                <div class="form-group">
                  <label>שם מוצר</label>
                  {{input class="form-control" value=item.name readonly="readonly"}}
                </div>
              </div>

              <div class="col-xs-3">
                <div class="form-group">
                  <label>תאריך מימוש</label>
                  {{input class="form-control" value=item.realizedOn readonly="readonly"}}
                </div>
              </div>

              <div class="col-xs-2">
                <div class="form-group">
                  <label>כמות שהוזמנה</label>
                  {{input class="form-control" value=item.qty readonly="readonly"}}
                </div>
              </div>

               <div class="col-xs-2">
                <div class="form-group">
                  <label>כמות מימוש</label>
                  {{input class="form-control" value=item.realizedQty readonly="readonly"}}
                </div>
              </div>

              <div class="col-xs-2">
                <div class="form-group">
                  <label>יתרה</label>
                  {{input class="form-control" value=item.left readonly="readonly"}}
                </div>
              </div>

          </div>
        {{/each}}
      {{/each}} 
      </div>
  </div>

</div>